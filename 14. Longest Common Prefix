class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        if len(strs) == 1:
            return strs[0]

        if not strs:
            return ""

        first_w = strs[0]

        for i in strs[1:]:
            while i[:len(first_w)] != first_w:
                first_w = first_w[:-1]

                if not first_w:
                    return ""

        return first_w
